{
  "expo": {
    "name": "e_commerce",
    "slug": "e_commerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./resources/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./resources/realicon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./resources/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lewishall.e_commerce"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon":"./resources/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ea06ee77-3e1d-4628-94a8-e6272ba0fbe4"
      }
    }
  }
}
